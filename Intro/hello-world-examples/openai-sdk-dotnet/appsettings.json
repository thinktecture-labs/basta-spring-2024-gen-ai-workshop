{
    "Serilog": {
      "MinimumLevel": {
        "Default": "Verbose",
        "Override": {
          "Microsoft": "Information",
          "Microsoft.OpenAI": "Debug"
        }
      },
      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": {
            "path": "Logs/log.txt"
          }
        }
      ],
      "Enrich": [
        "FromLogContext",
        "WithMachineName",
        "WithThreadId"
      ]
    },
    "Model": "gpt-4",
    "OpenAiApiKey": ""
}
